{
    "site_name": "App",
    "site_slogon": "",
    "site_root_url": "/AppDoc/",
    "site_domain": "IceSkyDev.github.io",
    "site_protocol": "https",
    "config_template_dir": "config",
    "source": "",
    "route": {
        "docs": {
            "/home/zh/": "docs/home/zh",
            "/edu/zh/": "docs/edu/zh",
            "/tools/zh/": "docs/tools/zh",
            "/game/zh/": "docs/game/zh",
            "/usage/zh/": "docs/usage/zh"
        },
        "pages": {
            "/": "pages/index/zh",
            "/en/": "pages/index/en"
        },
        "assets": {
            "/static/": "static",
            "/home/assets/": "docs/home/assets"
        },
        "blog": {
            "/blog/": "blog"
        }
    },
    "translate": {
        "docs": {
            "/home/zh/": [
                {
                    "url": "/home/en/",
                    "src": "docs/home/en"
                }
            ],
            "/edu/zh/": [
                {
                    "url": "/edu/en/",
                    "src": "docs/edu/en"
                }
            ],
            "/tools/zh/": [
                {
                    "url": "/tools/en/",
                    "src": "docs/tools/en"
                }
            ],
            "/game/zh/": [
                {
                    "url": "/game/en/",
                    "src": "docs/game/en"
                }
            ],
            "/usage/zh/": [
                {
                    "url": "/usage/en/",
                    "src": "docs/usage/en"
                }
            ]
        },
        "pages": {
            "/": [
                {
                    "url": "/en/",
                    "src": "pages/index/en"
                }
            ]
        }
    },
    "plugins": {
        "teedoc-plugin-markdown-parser":{
            "from": "pypi",
            "config": {
            }
        },
        "teedoc-plugin-theme-default":{
            "from": "pypi",
            "config": {
                "dark": true,
                "env":{
                    "main_color": "#228FF5"
                }
            }
        },
        "teedoc-plugin-search":{
            "from": "pypi",
            "config": {
                "search_hint": "Search"
            }
        },
        "teedoc-plugin-blog": {
            "from": "pypi",
            "config": {
            }
        },
        "teedoc-plugin-assets": {
            "from": "pypi",
            "config": {
                "header_items": [
                    "/static/css/custom.css",
                    "/static/css/bootstrap.min.css",
                    "/static/css/animate.min.css"
                ],
                "footer_items": [
                    "/static/js/custom.js",
                    "/static/js/easing.min.js"
                ],
                "env":{
                    "main_color": "#228FF5",
                    "second_color": "#195893"
                }
            }
        }
    },
    "rebuild_changes_delay": 1,
    "robots":{
        "User-agent": "*"
    },
    "layout_root_dir": "layout"
}
